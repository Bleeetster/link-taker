{
	"manifest_version": 3,
	"name": "link taker",
	"version": "1.1",
	"description": "Extension that allows you easily put your links in a list",
	"permissions": ["scripting" , "activeTab"],
	"action": {
		"default_popup": "./index.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"commands": {
		"SaveCurrentUrl": {
			"suggested_key": {
				"default": "Ctrl+S"
			},
			"description": "Saves current URL"
		}
	},
	"host_permissions": [ 
		"http://localhost:5439/"
	]
}
